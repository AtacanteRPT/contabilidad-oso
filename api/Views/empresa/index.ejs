<h1>empresa</h1>


<% if (result.length == 0) { %>
  <h4>
    0 empresa
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>createdAt</th>
			<th>updatedAt</th>
			<th>id</th>
			<th>nombre</th>
			<th>nit</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.createdAt %>
					</td>
					<td>
						<%= item.updatedAt %>
					</td>
					<td>
						<%= item.id %>
					</td>
					<td>
						<%= item.nombre %>
					</td>
					<td>
						<%= item.nit %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/empresa/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/empresa/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/empresa/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/empresa/create">New</a>