<h1>Asiento</h1>


<% if (result.length == 0) { %>
  <h4>
    0 Asiento
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>fecha</th>
			<th>detalle</th>
			<th>ref</th>
			<th>debe</th>
			<th>haber</th>
			<th></th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.fecha %>
					</td>
					<td>
						<%= item.detalle %>
					</td>
					<td>
						<%= item.ref %>
					</td>
					<td>
						<%= item.debe %>
					</td>
					<td>
						<%= item.haber %>
					</td>
					<td>
						<%= item. %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/Asiento/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/Asiento/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/Asiento/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/Asiento/create">New</a>